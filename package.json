    "humane-server": "./server.js"
  },
  "author": "Shailendra Sharma",
  "license": "MIT",
  "dependencies": {
    "babel-polyfill": "^6.9.0",
    "bluebird": "^3.4.0",
    "command-line-boilerplate": "^1.1.6",
    "config-boilerplate": "^1.1.5",
    "events": "^1.1.0",
    "expressjs-boilerplate": "^1.3.2",
    "humane-cockpit": "^1.3.11",
    "humane-data-pipeline": "^1.1.18",
    "humane-indexer": "^1.8.0",
    "humane-searcher": "^1.7.1",
    "immutable": "^3.8.1",
    "lodash": "^4.13.1",
    "md5": "^2.1.0",
    "mkdirp": "^0.5.1",
    "node-sass": "^3.7.0",
    "webpack-builder-boilerplate": "^1.2.0"
  },
  "devDependencies": {
    "babel-core": "^6.9.0",
    "babel-plugin-transform-runtime": "^6.9.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-node5": "^11.1.0",
    "babel-preset-react": "^6.5.0",
    "eslint": "^2.10.2",
    "eslint-config-360fy": "^1.1.6",
    "eslint-plugin-react": "^5.1.1",
    "webpack-builder-boilerplate": "^1.1.7"
  },
  "eslintConfig": {
