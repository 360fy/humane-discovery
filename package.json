{
  "name": "humane-discovery",
  "version": "1.9.4",
  "description": "Humane Discovery",
  "repository": "git@github.com:360fy/humane-discovery.git",
  "scripts": {
    "lint": "eslint ./src/",
    "clean": "rm -rf lib/",
    "compile": "npm run lint && npm run clean && babel -d lib/ src/ && webpack --progress --colors",
    "prepublish": "npm run compile"
  },
  "bin": {
    "humane-cli": "./cli.js",
    "humane-server": "./server.js"
  },
  "author": "Shailendra Sharma",
  "license": "LGPL",
  "dependencies": {
    "babel-polyfill": "6.9.1",
    "bluebird": "3.4.1",
    "command-line-boilerplate": "1.1.7",
    "config-boilerplate": "1.1.6",
    "events": "1.1.1",
    "expressjs-boilerplate": "1.3.9",
    "humane-cockpit": "1.3.15",
    "humane-indexer": "1.9.3",
    "humane-node-commons": "1.3.1",
    "humane-searcher": "1.8.3",
    "immutable": "3.8.1",
    "lodash": "4.13.1",
    "md5": "2.1.0",
    "mkdirp": "0.5.1",
    "node-sass": "3.8.0",
    "plugin-boilerplate": "1.1.0",
    "webpack-builder-boilerplate": "1.2.2"
  },
  "devDependencies": {
    "babel-core": "6.10.4",
    "babel-plugin-transform-runtime": "6.9.0",
    "babel-preset-es2015": "6.9.0",
    "babel-preset-node5": "11.1.0",
    "babel-preset-react": "6.11.1",
    "eslint": "3.1.1",
    "eslint-config-360fy": "1.1.10",
    "eslint-plugin-react": "5.2.2",
    "webpack-builder-boilerplate": "1.2.2"
  },
  "eslintConfig": {
    "extends": "360fy"
  },
  "babel": {
    "plugins": [
      "transform-runtime"
    ],
    "presets": [
      "node5",
      "react"
    ]
  }
}
