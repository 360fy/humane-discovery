{
  "name": "humane-discovery",
  "version": "1.0.0",
  "description": "Humane Discovery",
  "private": true,
  "scripts": {
    "lint": "eslint ./src/",
    "clean": "rm -rf lib/",
    "compile": "npm run lint && npm run clean && babel -d lib/ src/ && webpack --progress --colors",
    "prepublish": "npm run compile"
  },
  "bin": {
    "humane-cli": "./cli.js",
    "humane-server": "./server.js"
  },
  "author": "Shailendra Sharma",
  "license": "MIT",
  "dependencies": {
    "agentkeepalive": "^2.0.5",
    "babel-polyfill": "^6.7.2",
    "bluebird": "^3.3.4",
    "cli-boilerplate": "https://private-npm-releases.s3.amazonaws.com/cli-boilerplate-1.0.2.tgz",
    "config": "^1.19.0",
    "events": "^1.1.0",
    "expressjs-boilerplate": "https://private-npm-releases.s3.amazonaws.com/expressjs-boilerplate-1.0.9.tgz",
    "humane-cockpit": "https://private-npm-releases.s3.amazonaws.com/humane-cockpit-1.0.5.tgz",
    "humane-indexer": "https://private-npm-releases.s3.amazonaws.com/humane-indexer-1.0.5.tgz",
    "humane-searcher": "https://private-npm-releases.s3.amazonaws.com/humane-searcher-1.0.5.tgz",
    "immutable": "^3.7.6",
    "lodash": "^4.6.1",
    "md5": "^2.1.0",
    "moment": "^2.12.0",
    "request": "^2.69.0",
    "webpack-boilerplate": "https://private-npm-releases.s3.amazonaws.com/webpack-boilerplate-1.0.0.tgz"
  },
  "devDependencies": {
    "babel-core": "^6.7.2",
    "babel-plugin-transform-runtime": "^6.6.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-node5": "^11.0.0",
    "babel-preset-react": "^6.5.0",
    "eslint": "^2.4.0",
    "eslint-config-360fy": "https://private-npm-releases.s3.amazonaws.com/eslint-config-360fy-1.0.1.tgz",
    "eslint-plugin-react": "^4.2.3"
  },
  "eslintConfig": {
    "extends": "360fy"
  },
  "babel": {
    "plugins": [
      "transform-runtime"
    ],
    "presets": [
      "node5",
      "react"
    ]
  }
}
